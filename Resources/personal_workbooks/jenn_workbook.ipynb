    "# List of columns to sum the deaths\n",
    "death_columns = [col for col in df_mort.columns if col.startswith('Deaths') or col.startswith('IM_Deaths')]\n",
    "\n",
    "# Convert the relevant columns to numeric, coercing errors to NaN\n",
    "df_mort[death_columns] = df_mort[death_columns].apply(pd.to_numeric, errors='coerce')\n",
    "\n",
    "# Sum the columns to create the 'Total Deaths' column\n",
    "df_mort['Total Deaths'] = df_mort[death_columns].sum(axis=1)\n",
    "\n",
    "# Group by 'Country Name', 'Country Code', and 'Year' and sum the 'Total Deaths'\n",
    "df_grouped = df_mort.groupby(['Country Name', 'Country Code', 'Year'])['Total Deaths'].sum().reset_index()\n",
    "\n",
    "# Create a pivot table\n",
    "pivot_table = df_grouped.pivot_table(index=['Country Name', 'Country Code'], columns='Year', values='Total Deaths', aggfunc='sum')\n",
    "\n",
    "# Display the resulting pivot table\n",
    "display(pivot_table)"
   ]

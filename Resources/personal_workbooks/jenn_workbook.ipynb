    "# Create a new dataframe to summarize the death data without buckets\n",
    "\n",
    "# Assuming df_mort is already defined and loaded\n",
    "\n",
    "# List of columns to sum the deaths\n",
    "adult_deaths_cols = [col for col in df_mort_clean.columns if col.startswith('Deaths')]\n",
    "infant_deaths_cols = [col for col in df_mort_clean.columns if col.startswith('IM_Deaths')]\n",
    "\n",
    "# Convert the relevant columns to numeric, coercing errors to NaN\n",
    "df_mort_clean[adult_deaths_cols] = df_mort_clean[adult_deaths_cols].apply(pd.to_numeric, errors='coerce')\n",
    "df_mort_clean[infant_deaths_cols] = df_mort_clean[infant_deaths_cols].apply(pd.to_numeric, errors='coerce')\n",
    "\n",
    "# Sum the columns to create the 'Adult Deaths' and 'Infant Deaths' columns\n",
    "df_mort_clean['Adult Deaths'] = df_mort_clean[adult_deaths_cols].sum(axis=1)\n",
    "df_mort_clean['Infant Deaths'] = df_mort_clean[infant_deaths_cols].sum(axis=1)\n",
    "df_mort_clean['All Deaths'] = df_mort_clean['Adult Deaths'] + df_mort_clean['Infant Deaths']\n",
    "\n",
    "# Create final DataFrame\n",
    "df_summary = df_mort_clean[['Country Name','Adult Deaths', 'Sex', 'Infant Deaths', 'All Deaths']]\n",
